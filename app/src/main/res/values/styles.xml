    <style name="Text.Caption" parent="TextAppearance.AppCompat.Caption"></style>
    <style name="Text.Small" parent="TextAppearance.AppCompat.Small"></style>
    <style name="Text.Body1" parent="TextAppearance.AppCompat.Body1"></style>
